{
    "env": {
        "browser": true,
        "es2021": true
    },
    "extends": [
        "xo",
        "plugin:react/recommended"
    ],
    "overrides": [
        {
            "extends": [
                "xo-typescript"
            ],
            "files": [
                "*.ts",
                "*.tsx"
            ],
            "rules":{
                "react/prop-types": "off",
                "@typescript-eslint/triple-slash-reference": "off"
                
            }
        }
    ],
    "parserOptions": {
        "ecmaVersion": "latest",
        "sourceType": "module"
    },
    "plugins": [
        "react"
    ],
    "rules": {
    }
}
